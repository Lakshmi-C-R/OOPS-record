package com.company;
//CURRENCY CLASS
public class Main
{
    public double dollar_inr(double dollar)
    {
        return dollar*68;
    }
    public double inr_dollar(double inr)
    {
        return inr/68;
    }
    public double euro_inr(double euro)
    {
        return euro*78;
    }
    public double inr_euro(double inr)
    {
        return  inr/78;
    }
    public double yen_inr(double yen)
    {
        return yen*.6;
    }
    public double inr_yen(double inr)
    {
        return inr/.6;
    }
}
